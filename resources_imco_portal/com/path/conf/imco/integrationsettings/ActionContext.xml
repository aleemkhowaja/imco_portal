<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="integrationSettingsListAction" class="com.path.imco.actions.integrationsettings.IntegrationSettingsListAction" scope="prototype" parent="baseAction">
	 <property name="integrationSettingsBO" ref="integrationSettingsBO"></property>
	</bean>
	<bean id="integrationSettingsMaintAction" class="com.path.imco.actions.integrationsettings.IntegrationSettingsMaintAction" scope="prototype" parent="baseAction">
	 <property name="integrationSettingsBO" ref="integrationSettingsBO"></property>
	</bean>

	<bean id="syncBranchLookupAction" class="com.path.imco.actions.integrationsettings.SyncBranchLookupAction" scope="prototype" parent="baseAction">
	 <property name="integrationSettingsBO" ref="integrationSettingsBO"></property>
	</bean>
	<bean id="integrationEventMgmtAction" class="com.path.imco.actions.integrationsettings.IntegrationEventMgmtAction" scope="prototype" parent="baseAction">
	 <property name="integrationEventMgmtBO" ref="integrationEventMgmtBO"></property>
	</bean>

</beans>