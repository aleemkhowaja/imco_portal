<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fieldMapping" namespace="/path/fieldMapping" extends="path-default">
		<action name="FieldMappingListAction_*" class="fieldMappingListAction" method="{1}">
				<result name="success" type="json"> 
					<param name="ignoreHierarchy">false</param> 
				</result>
		</action>
		<action name="FieldMappingMaintAction_*" class="fieldMappingMaintAction" method="{1}">
				<result name="fieldMappingList">/WEB-INF/pages/imco/fieldmapping/FieldMappingMaint.jsp</result>
				<result name="success11">/WEB-INF/pages/imco/fieldmapping/FieldMappingMaint.jsp</result>
				<result name="syncMappingListGlobal">/WEB-INF/pages/imco/fieldmapping/SyncolMappingPopupGlobal.jsp</result>
				<result name="syncMappingListDefault">/WEB-INF/pages/imco/fieldmapping/SyncolMappingPopupDefault.jsp</result>
				<result name="syncMappingListFix">/WEB-INF/pages/imco/fieldmapping/SyncolMappingPopupFix.jsp</result>
				
				<result name="success" type="json"> 
					<param name="ignoreHierarchy">false</param> 
				</result>
				
		</action>

	</package>
</struts>