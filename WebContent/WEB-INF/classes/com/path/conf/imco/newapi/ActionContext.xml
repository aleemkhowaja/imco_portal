<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="newApiListAction" class="com.path.imco.actions.newapi.NewApiListAction" scope="prototype" parent="baseAction">
	 <property name="newApiBO" ref="newApiBO"></property>
	</bean>
	<bean id="newApiMaintAction" class="com.path.imco.actions.newapi.NewApiMaintAction" scope="prototype" parent="baseAction">
	 <property name="newApiBO" ref="newApiBO"></property>
	</bean>
	<bean id="newApiDependencyAction" class="com.path.imco.actions.newapi.NewApiDependencyAction" scope="prototype" parent="baseAction">
	 <property name="newApiBO" ref="newApiBO"></property>
	</bean>	


</beans>