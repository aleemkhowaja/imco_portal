<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="queueSettingsBO" factory-bean="imcoServiceBeanFactory"  factory-method="getInstance" parent="baseBO">					
		<constructor-arg name="implClassName" value="com.path.imco.bo.queuesettings.impl.QueueSettingsBOImpl" />
		<constructor-arg name="interfaceName" value="com.path.imco.bo.queuesettings.QueueSettingsBO" />
		<constructor-arg name="remoteServiceName" value="queueSettingsBOService" />
		<property name="queueSettingsDAO" ref="queueSettingsDAO"></property>
	</bean>	


</beans>